{
  "name": "@adjustmode1/internal-channel",
  "version": "3.0.6",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "files": [
    "src/protos",
    "src/typings",
    "src/interface",
    "swagger"
  ],
  "engines": {
    "node": ">=18 <19"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "swc --ignore \"**/*.spec.ts,src/hmr.ts\" --delete-dir-on-start --out-dir dist/ src/",
    "build-type": ".bin/build-types.sh",
    "swagger": ".bin/build-types.sh",
    "prepublishOnly": "sh .bin/republish.sh",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start -b swc -w",
    "start:debug": "nest start -b swc --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --detectOpenHandles --forceExit",
    "prepare": "[ \\\"$npm_config_production\\\" ] && exit 0; husky install"
  },
  "dependencies": {
    "@adjustmode1/proto-files": "3.0.1",
    "@amplication/opentelemetry-nestjs": "^4.3.7",
    "@grpc/grpc-js": "^1.8.11",
    "@grpc/proto-loader": "^0.7.5",
    "@halomeapis/halome-error-codes": "^0.2.27",
    "@halomeapis/nestjs-common-modules": "^0.4.2",
    "@metinseylan/nestjs-opentelemetry": "^3.0.0",
    "@nestjs/cli": "^10.1.18",
    "@nestjs/common": "^10.2.6",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.6",
    "@nestjs/cqrs": "^10.2.5",
    "@nestjs/elasticsearch": "^9.0.0",
    "@nestjs/microservices": "^10.2.6",
    "@nestjs/platform-express": "^10.2.6",
    "@opentelemetry/api": "^1.6.0",
    "@opentelemetry/exporter-jaeger": "^1.17.0",
    "@opentelemetry/instrumentation-fs": "^0.8.1",
    "@opentelemetry/instrumentation-grpc": "^0.43.0",
    "@opentelemetry/instrumentation-nestjs-core": "^0.33.1",
    "@opentelemetry/propagator-b3": "^1.17.0",
    "@opentelemetry/sdk-node": "^0.43.0",
    "@opentelemetry/sdk-trace-base": "^1.17.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "glob": "^11.0.0",
    "google-proto-files": "^3.0.3",
    "ioredis": "^5.3.2",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "mongodb": "^6.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-angular": "^17.7.0",
    "@commitlint/config-conventional": "^17.7.0",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.4",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.83",
    "@swc/jest": "^0.2.29",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/node": "^18.14.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "prettier": "^3.0.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "terser": "^5.19.4",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s?$": [
        "@swc/jest"
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
